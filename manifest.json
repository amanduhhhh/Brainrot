{
    "manifest_version": 3,
    "name": "Brainrot Soundalikes",
    "version": "1.1",
    "permissions": ["storage", "scripting", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "description": "Encircles meme soundalikes on webpages",
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
    ], 
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": ["<all_urls>"]
    

  }